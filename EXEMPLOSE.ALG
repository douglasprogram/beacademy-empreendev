Algoritmo "ExemploSe" //Nome do programa
VAR //Definição das variáveis
  nome : CARACTERE;
  salario : REAL;
INICIO
   escreva("Digite o nome: ")
   leia(nome)
   escreva("Digite o salario: ")
   leia(salario)
   se (salario>1200) entao
   escreval(nome, " seu salário é maior que o salário mínimo!!!")
   fimse
   Fimalgoritmo